	"bugs": {
		"url": "https://github.com/Rohit-KK15/SmarDeFi-Vault/issues"
	},
	"homepage": "https://github.com/Rohit-KK15/SmarDeFi-Vault/blob/master/README.md",
	"dependencies": {
		"@iqai/adk": "^0.5.7",
		"@openrouter/ai-sdk-provider": "^1.2.8",
		"cors": "^2.8.5",
		"dedent": "^1.7.0",
		"dotenv": "^17.2.2",
		"express": "^5.2.1",
		"node-cron": "^4.2.1",
		"zod": "^4.1.12"
	},
	"devDependencies": {
