	"dependencies": {
		"@iqai/adk": "^0.5.4",
		"@openrouter/ai-sdk-provider": "^1.2.8",
		"dedent": "^1.7.0",
		"dotenv": "^17.2.2",
		"node-cron": "^4.2.1",
		"zod": "^4.1.12"
	},
	"devDependencies": {
		"@types/node": "^24.3.1",
